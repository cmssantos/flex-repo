<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <PackageId>FlexRepo</PackageId>
    <Version>0.2.1</Version>
    <Authors>Cleudice Santos</Authors>
    <Company>Cleudice Santos</Company>
    <Product>FlexRepo</Product>
    <Description>A generic repository library</Description>
    <PackageTags>repository;generic;library</PackageTags>
    <PackageProjectUrl>https://github.com/cmssantos/flex-repo</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/cmssantos/flex-repo</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.5" />
  </ItemGroup>

</Project>
